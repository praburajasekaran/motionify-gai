{
  "name": "motionify-pm-portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:portal": "vite",
    "dev:landing": "npm run dev --prefix landing-page-new",
    "dev:functions": "netlify functions:serve -p 8888",
    "dev:portal-functions": "concurrently -n \"portal,functions\" -c \"cyan,green\" \"npm run dev:portal\" \"npm run dev:functions\"",
    "dev:all": "concurrently -n \"portal,landing,functions\" -c \"cyan,magenta,green\" \"npm run dev:portal\" \"npm run dev:landing\" \"npm run dev:functions\"",
    "build": "vite build",
    "build:landing": "npm run build --prefix landing-page-new",
    "build:all": "npm run build && npm run build:landing && npm run copy:portal",
    "copy:portal": "mkdir -p landing-page-new/.next/portal && cp -r dist/* landing-page-new/.next/portal/",
    "preview": "vite preview",
    "postinstall": "cd landing-page-new && npm install",
    "docs:api": "tsx shared/docs/generate-api-docs.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:payment": "playwright test --config=playwright.payment.config.ts",
    "test:e2e:payment:ui": "playwright test --config=playwright.payment.config.ts --ui",
    "test:e2e:payment:debug": "playwright test --config=playwright.payment.config.ts --debug",
    "db:migrate": "tsx database/migrate.ts up",
    "db:migrate:down": "tsx database/migrate.ts down",
    "db:migrate:status": "tsx database/migrate.ts status",
    "db:migrate:create": "tsx database/migrate.ts create",
    "test:load:api": "artillery run load-tests/api-load.yml",
    "test:load:frontend": "artillery run load-tests/frontend-load.yml",
    "test:load": "npm run test:load:api"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-bedrock-runtime": "^3.962.0",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@google/genai": "^1.30.0",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/node": "^10.37.0",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@types/d3": "^7.4.3",
    "@types/topojson-client": "^3.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.555.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "razorpay": "^2.9.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.5.1",
    "resend": "^6.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "topojson-client": "^3.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@netlify/functions": "^5.1.2",
    "@playwright/test": "^1.57.0",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^7.0.5",
    "@vitejs/plugin-react": "^5.0.0",
    "artillery": "^2.0.28",
    "artillery-engine-playwright": "^1.25.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "netlify-cli": "^23.13.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.19.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "zod-to-json-schema": "^3.24.1"
  }
}
